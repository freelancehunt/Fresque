{
  "name": "freelancehunt/fresque",
  "description": "A command line tool to manage your php-resque workers",
  "keywords": [
    "php-resque",
    "resque",
    "cli",
    "redis"
  ],
  "authors": [
    {
      "name": "Oleg Topchiy",
      "email": "edge@freelancehunt.com"
    },
    {
      "name": "Yaroslav Molchan",
      "email": "yaroslav@freelancehunt.com"
    },
    {
      "name": "Wan Chen",
      "email": "kami@kamisama.me",
      "homepage": "http://www.kamisama.me"
    }
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Fresque\\": "src/Fresque",
      "MonologInit\\": "src/MonologInit",
      "Redisent\\": "src/Redisent",
      "Resque\\": "src/Resque",
      "ResqueScheduler\\": "src/ResqueScheduler"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "ext-posix": "*",
    "ext-pcntl": "*",
    "ext-redis": "*",
    "zetacomponents/console-tools": "^1.7",
    "zetacomponents/base": "^1.9",
    "monolog/monolog": "^2.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "mikey179/vfsstream": "^1.6.10",
    "php-coveralls/php-coveralls": "^2.3"
  },
  "suggest": {
    "chrisboulton/php-resque": "PHP port of resque (Workers and Queueing)"
  },
  "support": {
    "email": "dev@freelancehunt.com",
    "issues": "https://github.com/freelancehunt/fresque/issues",
    "source": "https://github.com/freelancehunt/fresque"
  },
  "bin": [
    "bin/resque",
    "bin/fresque"
  ]
}
